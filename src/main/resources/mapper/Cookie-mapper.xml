<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.cloudstu.sg.dao.CookieDao">

    <select id="selectByDomain" parameterType="String" resultType="cookieModel">
        SELECT * FROM cookie
        WHERE domain = #{domain}
    </select>
</mapper>